{
	"id": "/kdc",
	"instances": 1,
	"cpus": 1,
	"mem": 128,
	"env": {
		"PRINCIPAL_hdfs": "hdfs@LOCAL:secret",
		"PRINCIPAL_tester": "tester@LOCAL:secret",
		"KEYTAB_hdfs": "hdfs@LOCAL:hdfs.keytab",
		"KEYTAB_tester": "tester@LOCAL:tester.keytab",
	},
	"container": {
		"type": "DOCKER",
		"docker": {
			"image": "sttts/kdc:latest",
			"network": "BRIDGE",
			"forcePullImage": true,
			"parameters": [
				{"key":"publish", "value": "88:88/udp"},
				{"key":"publish", "value": "88:88/tcp"}
			]
		}
	}
}
